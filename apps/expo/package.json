{
  "name": "@cai/expo",
  "version": "2.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "ios": "expo run:ios",
    "android": "expo run:android",
    "dev": "expo start --dev-client",
    "dev:prod": "expo start --no-dev --minify",
    "prebuild": "expo prebuild",
    "build:ios:preview": "eas build --platform ios --profile preview --local",
    "build:android:preview": "eas build --platform android --profile preview --local",
    "build:ios": "eas build --platform ios --profile production --local",
    "build:android": "eas build --platform android --profile production --local",
    "clean": "rm -rf .expo node_modules ios android .turbo",
    "lint": "echo 'No linting for Expo'",
    "typecheck": "tsc --noEmit",
    "format": "echo 'No formatting for Expo'"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@cai/ui-tw": "workspace:*",
    "@shopify/flash-list": "1.6.4",
    "app": "workspace:*",
    "babel-plugin-module-resolver": "^5.0.2",
    "burnt": "^0.13.0",
    "expo": "~53.0.11",
    "expo-apple-authentication": "~7.2.4",
    "expo-constants": "~17.1.6",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.0",
    "expo-font": "~13.3.1",
    "expo-image": "~2.3.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.1.0",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~5.0.8",
    "expo-updates": "~0.25.28",
    "expo-web-browser": "~14.1.6",
    "nativewind": "^4.1.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-mmkv": "^3.2.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@expo/metro-config": "~0.20.14",
    "metro-minify-terser": "^0.82.4",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3"
  }
}
