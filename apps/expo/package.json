{
  "name": "@cai/expo",
  "version": "2.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "ios": "expo run:ios",
    "android": "expo run:android",
    "dev": "expo start --dev-client",
    "dev:prod": "expo start --no-dev --minify",
    "prebuild": "expo prebuild",
    "build:ios:preview": "eas build --platform ios --profile preview --local",
    "build:android:preview": "eas build --platform android --profile preview --local",
    "build:ios": "eas build --platform ios --profile production --local",
    "build:android": "eas build --platform android --profile production --local",
    "clean": "rm -rf .expo node_modules ios android .turbo",
    "lint": "echo 'No linting for Expo'",
    "typecheck": "tsc --noEmit",
    "format": "echo 'No formatting for Expo'"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@shopify/flash-list": "^1.8.2",
    "@cai/ui-tw": "workspace:*",
    "app": "workspace:*",
    "babel-plugin-module-resolver": "^5.0.2",
    "burnt": "^0.12.2",
    "expo": "~51.0.0",
    "expo-apple-authentication": "~6.4.2",
    "expo-constants": "~16.0.2",
    "expo-crypto": "~13.0.2",
    "expo-dev-client": "~4.0.0",
    "expo-font": "~12.0.10",
    "expo-image": "~1.13.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.0",
    "expo-splash-screen": "~0.29.14",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.26.14",
    "expo-web-browser": "~13.0.3",
    "nativewind": "^4.1.23",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.75.4",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-mmkv": "^3.1.0",
    "react-native-safe-area-context": "^4.14.0",
    "react-native-screens": "^4.4.0",
    "react-native-svg": "^15.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@expo/metro-config": "~0.19.6",
    "metro-minify-terser": "^0.81.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  }
}
