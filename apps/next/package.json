{
  "name": "@cai/web",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "local-build": "bun x @cloudflare/next-on-pages --disable-worker-minification",
    "local-serve": "bun x wrangler pages dev .vercel/output/static --port 3000 --compatibility-date 2024-12-18 --compatibility-flag=nodejs_compat",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next node_modules .turbo"
  },
  "dependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@cai/ui-tw": "workspace:*",
    "@tsndr/cloudflare-worker-jwt": "^3.1.3",
    "app": "workspace:*",
    "million": "^3.1.11",
    "next": "^15.1.3",
    "next-seo": "^6.6.0",
    "next-superjson-plugin": "^0.5.10",
    "pattycake": "^0.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "webpack": "^5.97.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.2",
    "tailwindcss": "^3.4.17",
    "vercel": "^39.2.5",
    "wrangler": "^4.19.1"
  }
